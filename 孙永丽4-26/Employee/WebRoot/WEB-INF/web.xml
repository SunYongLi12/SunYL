<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>USerServlet</servlet-name>
    <servlet-class>cn.hfxt.servlet.USerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>MenuServlet</servlet-name>
    <servlet-class>cn.hfxt.servlet.MenuServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FlowerServlet</servlet-name>
    <servlet-class>cn.hfxt.servlet.FlowerServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>FloServlet</servlet-name>
    <servlet-class>cn.hfxt.servlet.FloServlet</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>USerServlet</servlet-name>
    <url-pattern>/USerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MenuServlet</servlet-name>
    <url-pattern>/MenuServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FlowerServlet</servlet-name>
    <url-pattern>/FlowerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>FloServlet</servlet-name>
    <url-pattern>/FloServlet</url-pattern>
  </servlet-mapping>

</web-app>